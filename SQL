1/ SELECT * FROM EMP
WHERE COMM IS NOT NULL
ORDER BY COMM ASC ;

2/ SELECT DISTINCT NOME FROM EMP
WHERE DATE>=01-09-2020 ;

3/ SELECT * FROM EMP 
WHERE EMBAUCHE='AZLI';


4/ SELECT * FROM EMP 
WHERE NOME='AZLI';

5/ SELECT AVG(SALAIRE)
FROM EMP;


6/ SELECT COUNT(COMM)
FROM EMP
WHERE COMM IS NOT NULL;


7/ SELECT * FROM EMP
WHERE SALAIRE > (SELECT AVG(SALAIRE) FROM EMP);
